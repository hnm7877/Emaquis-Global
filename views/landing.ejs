    <!-- WhatsApp Assistant JavaScript -->
    <script>
      // WhatsApp Assistant functionality
      document.addEventListener("DOMContentLoaded", function () {
        const whatsappHelpBtn = document.getElementById("whatsappHelpBtn");
        const whatsappModal = document.getElementById("whatsappModal");
        const whatsappModalClose =
          document.getElementById("whatsappModalClose");
        const whatsappNumbers = document.querySelectorAll(".whatsapp-number");

        // Ouvrir le modal
        whatsappHelpBtn.addEventListener("click", function () {
          whatsappModal.style.display = "block";
          this.classList.add("clicked");
          setTimeout(() => {
            this.classList.remove("clicked");
          }, 600);
        });

        // Fermer le modal
        whatsappModalClose.addEventListener("click", function () {
          whatsappModal.style.display = "none";
        });

        // Fermer le modal en cliquant à l'extérieur
        whatsappModal.addEventListener("click", function (e) {
          if (e.target === whatsappModal) {
            whatsappModal.style.display = "none";
          }
        });

        // Gérer les clics sur les numéros WhatsApp
        whatsappNumbers.forEach((number) => {
          number.addEventListener("click", function () {
            const phoneNumber = this.getAttribute("data-number");
            const contactName = this.getAttribute("data-name");
            const message = encodeURIComponent(
              `Bonjour ! Je suis intéressé(e) par E-Maquis et j'aimerais avoir plus d'informations. Merci !`
            );
            const whatsappUrl = `https://wa.me/${phoneNumber}?text=${message}`;

            // Ouvrir WhatsApp
            window.open(whatsappUrl, "_blank");

            // Fermer le modal
            whatsappModal.style.display = "none";
          });
        });

        // Fermer avec la touche Escape
        document.addEventListener("keydown", function (e) {
          if (e.key === "Escape" && whatsappModal.style.display === "block") {
            whatsappModal.style.display = "none";
          }
        });
      });
    </script>
