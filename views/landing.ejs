            userQueries
              .getAllUSers()
              .then((data) => {
                if (data.etat) {
                  resolve(data.result);
                } else {
                  reject(data.err);
                }
              })
              .catch((err) => {
                reject(err);
              });
          });

          console.log("All Users:", result);
        } catch (error) {
          console.error("Error importing users:", error);
        }
      }

